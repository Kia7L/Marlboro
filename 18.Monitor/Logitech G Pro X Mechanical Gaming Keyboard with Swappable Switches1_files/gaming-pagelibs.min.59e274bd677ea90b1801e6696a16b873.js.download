		['currentTab', this.currentTab],
	  	['type', type],
		['s', 'g'],
		['st', 1],
		['m', 12],
		['f', 's'],
		['lang2', lang2],
		['lang_country', htmlLang.replace('-', '_')],
		['lang_country_lower', htmlLang.replace('-', '_').toLowerCase()]
	];

	for (var i = 0; i < paramsArray.length; i++) {
		var paramName = paramsArray[i][0];
		var defaultValue = paramsArray[i][1];

		if (overrides.hasOwnProperty(paramName)) {
			// Use override value if provided
			this.searchParams[paramName] = overrides[paramName];

		} else if (this.urlVars.hasOwnProperty(paramName)) {
			// Otherwise use url value if present
			var tempVal = this.urlVars[paramName];
			tempVal = tempVal.split('#');
			tempVal = tempVal[0];
			this.searchParams[paramName] = tempVal;

		} else {
			// Otherwise fall back to default value
			this.searchParams[paramName] = defaultValue;

		}
	}
};


SearchResults.prototype.buildQueryString = function(paramsObj) {
	if (typeof(paramsObj) === 'undefined') {
		paramsObj = {};
	}

	var mergedParams = $.extend({}, this.searchParams, paramsObj);
	var queryString = '';

	queryString += '?q='          + encodeURIComponent(mergedParams.q);
	queryString += '&s='          + encodeURIComponent(mergedParams.s);
	queryString += '&lang='       + encodeURIComponent(mergedParams.lang);
	queryString += '&m='          + encodeURIComponent(mergedParams.m);
	queryString += '&f='          + encodeURIComponent(mergedParams.f);
	queryString += '&st='         + encodeURIComponent(mergedParams.st);
	queryString += '&type='       + encodeURIComponent(mergedParams.type);
	queryString += '&currentTab=' + encodeURIComponent(this.currentTab);

	return queryString;
};


SearchResults.prototype.initNav = function(e) {
	this.$navBtns.click(function(e) {
		e.preventDefault();
		var $obj = $(e.currentTarget);
		var newTabName = $obj.attr('data-tab-name');
		var delay = 0;



		if (this.breakpoint !== 'desktop') {
			// Switch between mobile tabs

			if (this.currentTab !== SearchResults.TAB_NONE) {
				this.closeMobileTab(this.currentTab);
			}

			if (this.currentTab !== newTabName && this.currentTab !== SearchResults.TAB_NONE) {
				delay = this.mobileAnimDuration * 1000;

			} else if (this.currentTab === newTabName) {
				this.currentTab = SearchResults.TAB_NONE;
				this.updateUrl({
					st: 1,
					currentTab: SearchResults.TAB_NONE
				});

				return;
			}
		}


		this.updateUrl({
			st: 1,
			currentTab: newTabName
		});

		setTimeout(function() {
			this.switchToTab(newTabName);
		}.bind(this), delay);

	}.bind(this));

	if (this.currentTab !== SearchResults.TAB_NONE) {
		this.switchToTab(this.currentTab);
	}
};


SearchResults.prototype.closeMobileTab = function(tabName) {
	var $currentTab = this.$resultTabs.filter('[data-tab-name="' + tabName + '"]');
	var $inner = $currentTab.find('> div');

	TweenLite.to($inner, this.mobileAnimDuration, {height: 0, ease:Power1.easeOut, onComplete: function() {
			$inner.removeAttr('style');
			this.toggleResults(false);
		}.bind(this)
	});
	$currentTab.removeClass('active');
};


SearchResults.prototype.switchToTab = function(newTabName) {
	if (newTabName === this.currentTab && this.intialTabSwitchComplete) {
		return;
	}

	this.toggleRequestErrorMessage(false);

	var $parent = this.$navBtns.filter('[data-tab-name="' + newTabName + '"]').parent();
	var $outgoingTab = this.$resultTabs.filter('[data-tab-name="' + this.currentTab + '"]');
	var $incomingTab = this.$resultTabs.filter('[data-tab-name="' + newTabName + '"]');

	if (!$parent.hasClass('active')) {
		this.$listItems.removeClass('active');
		$parent.addClass('active');
	}

	$outgoingTab.removeClass('active');


	$incomingTab.append(this.$loadingAnim);
	this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.SHOW);


	$incomingTab.addClass('active');

	this.currentTab = newTabName;

	// Restart at page 1 when switching tabs
	if (this.intialTabSwitchComplete) {
		this.searchParams.st = 1;
		this.updateUrl({
			st: this.searchParams.st
		});
	}

	this.populateTab();

	this.intialTabSwitchComplete = true;
};


SearchResults.prototype.populateTab = function() {
	if (this.currentTab === 'products' || this.currentTab === 'information') {
		// Products & Information fetch data from our own endpoints

		if (this.resultsData.hasOwnProperty(this.currentTab) && this.resultsData[this.currentTab].hasOwnProperty(1)) {
			// Data already loaded & elements built. Show the first page of results
			this.$window.trigger(GLOBALS.searchResults.api.REPLACE_DATA, [this.currentTab, this.resultsData[this.currentTab][1]]);
			this.buildPagination();

			this.toggleResults(true);

		} else {
			// Data not loaded & elements not built for current tab. Make it happen
			if (!this.loadingData) {
				this.getResultData();
			}
		}

	} else if (this.currentTab === 'downloads' || this.currentTab === 'support') {

		this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.SHOW);
		this.toggleResults(false);
		this.hidePagination();

		// Downloads & Support sections use the Coveo JS Search Framework
		var urlIsSet            = this.initSupportDomain();
		var tokenIsSet          = this.initToken();
		var coveoScriptIsLoaded = this.loadCoveoScript();

		$.when(urlIsSet, tokenIsSet, coveoScriptIsLoaded).done(function() {
			this.doCoveoSearch();
		}.bind(this));

		this.$coveoRoot.appendTo('.js-results-tab[data-tab-name="' + this.currentTab + '"]');
	}
};


SearchResults.prototype.doCoveoSearch = function() {
	if (!this.coveoIsInitialized) {
		this.initCoveo();
	} else {
		Coveo.executeQuery(this.$coveoRoot[0]);
	}
};


SearchResults.prototype.toggleResults = function(val) {
	if (val) {
		// Show results
		this.$resultTabs.filter('[data-tab-name="' + this.currentTab + '"]').children().removeClass('hidden');
		this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.HIDE);
	} else {
		// Hide results
		this.$resultTabs.each(function(index, el) {
			$(el).children().not('.search-results-nav-item.mobile, .js-g-loading-anim, .js-request-failure').addClass('hidden');
		}.bind(this));
	}
};


SearchResults.prototype.buildCoveoSearchQuery = function(qBuilder) {
	var aq;
	var cq;
	var dq;
	var overrides = {};

	// Make sure searchParams object is up to date
	this.buildSearchParams(overrides, true);

	var q                  = this.searchParams.q;
	var lang2              = this.searchParams.lang2;
	var language           = this.searchParams.language;
	var lang_country_lower = this.searchParams.lang_country_lower;

	if (this.currentTab === 'support') {
		// Support
		aq = [
			'(',
				'(',
					'@objecttypename==Product',
					'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
					'@sftranslatedproductcsearchproducturlpathc',
					'@sftranslatedproductcsearchcountrylanguagecountrycodec==("' + lang_country_lower + '")',
				')',

				'OR (@objecttype==("FeedItem","FeedComment"))',

				'OR (',
					'(',
						'@objecttype==("kbpls")',
						'@sflanguage=("' + lang2 + '")',
						'@sfid=[',
							'[@sfknowledgearticleidc]',
							'@objecttype==("productarticlec")',
							'[',
								'[@lcommonproductid]',
								'@objecttype==("Product")',
								'@sftranslatedproductcsearchproductpublishdatec <now',
							']',
						']',
					')',
					'OR (',
						'@objecttype==("KBFAQ")',
						'@sflanguage=("' + lang2 + '")',
					')',
				')',
			')',
			'(',
				'$qre(expression:\'@sftranslatedproductcuntranslatedseotitlec="' + q + '" OR @sftranslatedproductctranslatedshortnamec="' + q + '"\', modifier:100)',
			')'
		].join(' ');


		cq = [
			'(',
				'NOT @objecttype==(User,CaseComment,Case,Attachment)',
				'(',
					'NOT @objecttype==(kbfaq)',
					'OR (',
						'@SfKbValidationStatus==(validated_external)',
						'@SfKbWebProductPublishDateC<now',
					')',
				')',
				'(',
					'NOT @sftranslatedproductcsearchproductpublishdatec',
					'OR @sftranslatedproductcsearchproductpublishdatec<now',
				')',
				'(',
					'NOT @sftranslatedproductcsearchproductstatusc',
					'OR @sftranslatedproductcsearchproductstatusc=Published',
				')',
				'(',
					'NOT @sftranslatedproductcstatusc',
					'OR @sftranslatedproductcstatusc==Approved',
				')',
				'NOT @liboardtitle',
			')',
			'(',
				'(',
					'NOT @sftranslatedproductcsearchcountrylanguagecountrycodec',
					'OR @sftranslatedproductcsearchcountrylanguagecountrycodec==(' + lang_country_lower + ')',
				')',
				'NOT @objecttype==TranslatedProductC',
				'NOT @sfrecordtypename==Image',
			')'
		].join(' ');


		dq = [
			'(',
				'@objecttypename==Product',
				'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
				'@sftranslatedproductcsearchproducturlpathc',
				'@sftranslatedproductcsearchcountrylanguagecountrycodec==(' + lang_country_lower + ')',
				'[',
					'[@lcommonproductlineid] ' + q + ' @objecttype==(X9LevelC)',
				']',
			')',
			'OR (',
				'@objecttype==Software',
				'@sftranslatedproductcsearchcountrylanguagecountrycodec==(' + lang_country_lower + ')',
				'@lcommonproductid=[',
					'[@sfwebproductsupportproductid]',
					'[',
						'[@sfsoftwaredocsimageid]',
						'@sfwebproductsupportproductid=[',
							'[@lcommonproductid]',
							'PartialMatch (keywords=\'' + q + '\'; match=100%; pick=50%; stopWords=; noRanking=false; noHighlight=false)',
							'@objecttype==Product',
						']',
					']',
				']',
			')'
		].join(' ');


	} else if (this.currentTab === 'downloads') {
		// Downloads
		aq = [
			'(',
				'(',
					'@objecttypename=="Product"',
					'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
					'@sftranslatedproductcsearchproducturlpathc',
					'@sftranslatedproductcsearchcountrylanguagecountrycodec==("' + lang_country_lower + '")',
				')',

				'OR (',
					'@objecttypename=="Software"',
					'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
					'@sftranslatedproductcsearchcountrylanguagecountrycodec==("' + lang_country_lower + '")',
				')',

				'(',
					'NOT @sftranslatedproductcsearchproductexternalurlc',
				')',

				'OR (',
					'(',
						'@objecttypename==Software',
						'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
						'@sftranslatedproductcsearchcountrylanguagecountrycodec==(' + lang_country_lower + ')',
					')',
					'NOT @sftranslatedproductcsearchproductexternalurlc',
				')',
			')',
			'(',
				'$qre(expression:\'@sftranslatedproductcuntranslatedseotitlec="' + q + '" OR @sftranslatedproductctranslatedshortnamec="' + q + '"\', modifier:100)',
			')'
		].join(' ');


		cq = [
			'(',
				'@objecttypename=="Product"',
				'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
				'@sftranslatedproductcsearchcountrylanguagecountrycodec==("' + lang_country_lower + '")',
			')',
			'OR (',
				'@objecttypename==Software',
				'@sftranslatedproductcsearchbusinessgroupc==(Gaming)',
				'@sftranslatedproductcsearchcountrylanguagecountrycodec==("' + lang_country_lower + '")',
			')'
		].join(' ');


		dq = [
			'(',
				'@objecttype==Software',
				'@sftranslatedproductcsearchcountrylanguagecountrycodec==("' + lang_country_lower + '")',
				'@lcommonproductid=[',
					'[@sfwebproductsupportproductid]',
					'[',
						'[@sfsoftwaredocsimageid]',
						'@sfwebproductsupportproductid=[',
							'[@lcommonproductid]',
							'(',
								'$some(keywords:\'' + q + '\', best:50%, match:100%)',
							')',
							'@objecttype==Product',
						']',
					']',
				']',
			')'
		].join(' ');
	}

	qBuilder.expression.add(this.searchParams.q);
	qBuilder.advancedExpression.add(aq);
	qBuilder.constantExpression.add(cq);
	qBuilder.disjunctionExpression.add(dq);
	qBuilder.addContextValue('languageCountryCode', lang2);

	qBuilder.enableDebug = false;
};


SearchResults.prototype.initCoveo = function(overrides) {
	Coveo.SearchEndpoint.configureCloudEndpoint(this.coveoOrgName, this.searchToken);

	// this.$coveoRoot.on('newQuery', function(e, args) {
	// 	console.log('>>> coveo.newQuery');
	// 	console.log(args);
	// }.bind(this));

	this.$coveoRoot.on('buildingQuery', function(e, args) {
		// console.log('>>> coveo.buildingQuery');
		// console.log(args);

		this.buildCoveoSearchQuery(args.queryBuilder);

	}.bind(this));

	// this.$coveoRoot.on('doneBuildingQuery', function(e, args) {
	// 	console.log('>>> coveo.doneBuildingQuery');
	// 	console.log(args);
	// }.bind(this));

	this.$coveoRoot.on('querySuccess', function(e, args) {
		this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.HIDE);


		if (args.results.totalCountFiltered === 0) {
			this.toggleNoResultsMessage(true);
		} else {
			this.toggleNoResultsMessage(false);
		}

		if (args.results.totalCountFiltered > this.coveoResultsPerPage) {
			this.toggleCoveoPager(true);
		} else {
			this.toggleCoveoPager(false);
		}


		this.jumpToTopResult();


		this.toggleResults(true);

		DataLayer.set('onsitesearch_query', args.query.q);

	}.bind(this));


	this.$coveoRoot.on('preprocessResults', function(e, args) {

		args.results.results.forEach(function(r) {
			var clickUriModified = false;

			// Knowledge content
            if(r.raw.kbid){
                r.clickUri = CoveoLib.getKBClickUri(r, this.supportDomain, this.searchParams.lang_country_lower);
				clickUriModified = true;
            }

            // Product content
            if (r.raw.sftranslatedproductcsearchproducturlpathc && r.raw.sftranslatedproductcsearchproductrecordtypenamec==='Product' && !r.raw.sftranslatedproductcsearchproductexternalurlc) {
				r.clickUri = CoveoLib.getProductPageClickUri(r, this.supportDomain, this.searchParams.lang_country_lower);
                if (this.searchParams.q) {
                    if (this.searchParams.q.toLowerCase().indexOf('download') > -1 || this.searchParams.q.toLowerCase().indexOf('software') > -1 ) {
                        r.clickUri = r.clickUri + '#download';
                    }
                }
                if (this.currentTab === 'downloads') {
                    //r.clickUri = r.clickUri + '#imagegallery';
					r.clickUri = r.clickUri + '/downloads';
                    r.raw.sftranslatedproductctranslatedproductnamewpc += ' - Download Page';
                    r.raw.sfuntranslatedproductctranslatedproductnamewp += ' - Download Page';
                }
				clickUriModified = true;
            }

            // Product with external url
            if (r.raw.sftranslatedproductcsearchproducturloverridec && r.raw.sftranslatedproductcsearchproductexternalurlc) {
                 r.clickUri = r.raw.sftranslatedproductcsearchproductexternalurlc;
 				clickUriModified = true;
            }

            // Downloads (software)
            if (r.raw.sftranslatedproductcsearchproducturlpathc && r.raw.sftranslatedproductcsearchproductrecordtypenamec==='Software') {
				r.clickUri = CoveoLib.getSoftwareClickUri(r, this.supportDomain, this.searchParams.lang_country_lower);
				clickUriModified = true;
            }

			// Default / other
			if (!clickUriModified) {

			}

		}.bind(this));

	}.bind(this));


	this.$coveoRoot.on('queryError', function(e, args) {
		console.log('>>> coveo.queryError');
		// console.log(args);
		this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.HIDE);
	}.bind(this));

	var options = {
		SearchInterface: {
			enableDuplicateFiltering: true
		},
		ResultList: {
			autoSelectFieldsToInclude: false,
		}
	}

	Coveo.init(this.$coveoRoot[0], options);

	this.coveoIsInitialized = true;
}


SearchResults.prototype.getResultData = function(overrides) {
	var searchUrl = this.buildSearchUrl(overrides);
	var $targElm;

	switch (this.currentTab) {
		case 'products':
			$targElm = this.$productsResultsTarg;
			break;
		case 'information':
			$targElm = this.$infoResultsTarg;
			break;
	}


	this.loadingData = true;

	this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.SHOW);

	this.toggleNoResultsMessage(false);

	$.getJSON(searchUrl, function(data) {

		if (!data.hasOwnProperty('counters')) {
			this.toggleNoResultsMessage(true);
			return;
		}

		// Store the loaded data so we don't have to re-request it
		if (!this.resultsData.hasOwnProperty(this.currentTab)) {
			this.resultsData[this.currentTab] = {};
		}
		this.resultsData[this.currentTab][data.counters.currpage] = data;


		// Don't build a new resultsTab object here if coming back to a previously-loaded page.
		if (!this.resultsTabObjects.hasOwnProperty(this.currentTab)) {
			// console.log('resultsTabObject does not exist for', this.currentTab);
			this.resultsTabObjects[this.currentTab] = new ResultsTab($targElm, data, this.currentTab);
		} else {
			// console.log('resultsTabObject exists for', this.currentTab, ', update its contents...');
			this.$window.trigger(GLOBALS.searchResults.api.REPLACE_DATA, [this.currentTab, data]);
		}

		this.updateUrl({
			st: data.counters.currpage,
			currentTab: this.currentTab
		});
		this.buildPagination();

	}.bind(this))
		.always( this.requestAlways.bind(this) )
		.fail( this.requestFailure.bind(this) );

};


SearchResults.prototype.toggleNoResultsMessage = function(val) {
	if (val) {
		var $currentTab = this.$resultTabs.filter('[data-tab-name="' + this.currentTab + '"]');
		this.$noResults.appendTo($currentTab).show();
	} else {
		this.$noResults.appendTo(this.$resultsCtn).hide();
	}
};


SearchResults.prototype.toggleCoveoPager = function(val) {
	var $coveoPager = this.$elm.find('.coveo-pager-list');
	if (val) {
		$coveoPager.show();
	} else {
		$coveoPager.hide();
	}
};


SearchResults.prototype.toggleRequestErrorMessage = function(val) {
	if (val) {
	    var $currentTab = this.$resultTabs.filter('[data-tab-name="' + this.currentTab + '"]');
	    this.$requestFailure.removeClass('hidden').appendTo($currentTab).show();
	} else {
		this.$requestFailure.appendTo(this.$resultsCtn).hide();
	}
};


SearchResults.prototype.requestAlways = function(data) {
	this.loadingData = false;
	this.$loadingAnim.trigger(GLOBALS.loadingAnim.api.HIDE);
	this.toggleResults(true);

	DataLayer.set('onsitesearch_query', data.query);
};


SearchResults.prototype.requestFailure = function(jqxhr, textStatus, error) {
   console.error('request failure:', error);
   this.toggleResults(false);
   this.toggleRequestErrorMessage(true);
};


SearchResults.prototype.hidePagination = function() {
	this.$pagination.removeClass('active');
};


SearchResults.prototype.buildPagination = function() {
	var counters = null;
	try {
		counters = this.resultsData[this.currentTab][this.searchParams.st].counters;
	} catch(e) {
		// No pagination possible or required if no counters
		return;
	}
	var links = '';
	var lastPage = parseInt(counters.lastpage, 10);
	var currPage = parseInt(counters.currpage, 10);
	var $prevNext = $.fn.add.call(this.$prevPageLink, this.$nextPageLink);

	if (lastPage === 1) {
		// Don't show or create pagination if only 1 page
		this.$pagination.removeClass('active');
		return;
	} else if (lastPage > 1) {
		// Create & show pagination if more than 1 page
		this.$pagination.addClass('active');
	}


	var baseUrl = window.location.protocol + '//' + window.location.host + window.location.pathname;
	var qs = '';

	var minPageNum = 1;
	var maxPageNum = lastPage;

	// If not on desktop, limit the number of page links created
	if (this.breakpoint !== 'desktop') {
		var maxPageLinks = 4;
		minPageNum = Math.max(currPage - 2, 1);
		maxPageNum = Math.min(currPage + 2, lastPage);

		// Always show at least 1 prev page if available
		if (currPage > 2 && lastPage > 4) {
			minPageNum = currPage - 1;
		}

		// Pad the previous page links if needed
		if (lastPage - currPage < 2 && (maxPageNum - minPageNum) < maxPageLinks) {
			while (minPageNum > 2 && (maxPageNum - minPageNum) < (maxPageLinks - 1)) {
				minPageNum--;
			}
		}

		// Pad the next page links if needed
		while (currPage <= 2 && lastPage > 3 && (maxPageNum - minPageNum) < (maxPageLinks - 1)) {
			maxPageNum++;
		}
	}



	// Individual page links
	for (var i = minPageNum; i <= maxPageNum; i++) {
		var newPageNum = i;
		var activeClass = '';
		if (newPageNum === currPage) {
			activeClass = 'active';
		}
		qs = this.buildQueryString({ st: newPageNum});
		links += '<a href="' + qs + '" class="page-link ' + activeClass +'" data-page-num="' + newPageNum + '" data-tab-name="' + this.currentTab + '">' + newPageNum + '</a>';
	}

	this.$pageLinksCtn.find('.page-link').remove();
	$(links).insertBefore(this.$nextPageLink);

	// Prev page
	if (currPage > 1) {
		var prevPageNum = currPage - 1;
		qs = this.buildQueryString({ st: prevPageNum});
		this.$prevPageLink
			.attr('title', this.previousCopy)
			.attr('href', baseUrl + qs)
			.attr('data-page-num', prevPageNum)
			.addClass('active');
	} else {
		this.$prevPageLink.removeClass('active');
	}

	// Next page
	if (currPage < lastPage) {
		var nextPageNum = currPage + 1;
		qs = this.buildQueryString({ st: nextPageNum});
		this.$nextPageLink
			.attr('title', this.nextCopy)
			.attr('href', baseUrl + qs)
			.attr('data-page-num', nextPageNum)
			.addClass('active');
	} else {
		this.$nextPageLink.removeClass('active');
	}


	// Ajax loading makes things nicer
	this.$pageLinksCtn.find('.page-link').click(function(e) {
		this.toggleResults(false);
		this.jumpToTopResult();

		var $obj = $(e.currentTarget);
		this.loadNewResultsPage($obj.attr('data-page-num'));
		this.updateUrl({
			st: $obj.attr('data-page-num')
		});
		$obj.addClass('active').siblings().removeClass('active');

		e.preventDefault();
	}.bind(this));

	$prevNext.off('click').click(function(e) {
		this.toggleResults(false);
		this.jumpToTopResult();

		var $obj = $(e.currentTarget);
		var newPageNum = $obj.attr('data-page-num');
		var $pageLink = this.$pageLinksCtn.find('.page-link[data-page-num="' + newPageNum + '"]');
