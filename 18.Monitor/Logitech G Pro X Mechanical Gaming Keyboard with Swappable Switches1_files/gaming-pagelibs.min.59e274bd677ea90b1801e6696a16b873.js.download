	this.hasModelChoices = this.$modelDropdowns.length > 0;

	if(this.hasColorChoices) {
		this.currentColor = this.$colorSwatches.first().attr('data-color-id');

		if(this.$modelDropdowns.length > 0) {
			this.updateModelDropdowns2(this.currentColor);
		}
	}

	if(this.selector !== null && this.selector !== undefined && this.numVariants > 1) {
		this.deepLinkTo(this.selector.trim());
	}

	this.updatePriceAndCta();

	this.updateProductShots();


	// If we're in a modal, immediately see if we can track the user viewing a single SKU
	this.allowTrackSingleSKU = true;
	if(this.isModal) {
		Analytics.trackBuyModuleAsModal(this.productInfo.uniqueId, this.productInfo.category, this.productInfo.title);
		this.attemptSingleSkuAnalytics();

		// Otherwise, we wait for the user to scroll the buy module on-screen before we start tracking single-SKU views
	} else if(this.scrollMagicController) {
		var scene = new ScrollMagic.Scene( {triggerElement: this.$elm[0], triggerHook:0.5} );
		scene.on( 'enter', function() {
			this.attemptSingleSkuAnalytics();
			scene.destroy();
		}.bind(this) );
		scene.addTo(this.scrollMagicController);
	}

	this.onResize();
	this.$colorControls.on( GLOBALS.colorSwatches.events.CHANGED, this.onChangeColor.bind(this) );
	this.$modelDropdowns.change( this.onChangeModel.bind(this) );
	this.$window.on( GLOBALS.miniCart.events.ADDING_TO_CART, this.onAddingToCart.bind(this) );
	this.$cta.on( GLOBALS.productCta.events.CLICKED_ADD_TO_CART, this.clickedAddToCart.bind(this) );
	this.$modalLink.click( this.openExtraInfoModal.bind(this) );
	this.$window.on( GLOBALS.events.WIN_RESIZE, this.onResize.bind(this) );
	this.$window.on( GLOBALS.bundles.events.COLOR_CHANGE, this.onExternalColorChange.bind(this) );
	this.$window.on( GLOBALS.bundles.events.MODEL_CHANGE, this.onExternalModelChange.bind(this) );
};



BuyModule.prototype.setProductInfo = function(productPath, productInfo) {
	if(productPath !== undefined && productPath !== null && productPath.trim() !== '') {
		this.store.setProductInfoFor(productPath, productInfo)
			.then(function(productInfo) {
				// Failsafe to make sure we're actually receiving the data we're expecting
				if(typeof productInfo !== 'object' || productInfo.variants === undefined || productInfo.variants === null) {
					console.warn('buy-module failed on ', productPath);
					console.warn('it received', productInfo);
					return;
				};
				this.productInfo = JSON.parse(JSON.stringify(productInfo));
				this.init();

			}.bind(this))
			.fail(function(error) {
				console.warn('buy-module failed on ', productPath);
				console.error(error);
			});
	}
};


BuyModule.prototype.getProductInfo = function(productPath) {
	if(productPath !== undefined && productPath !== null && productPath.trim() !== '') {
		this.store.getProductInfoFor(productPath)
			.then(function(productInfo) {
				// Failsafe to make sure we're actually receiving the data we're expecting
				if(typeof productInfo !== 'object' || productInfo.variants === undefined || productInfo.variants === null) {
					console.warn('buy-module failed on ', productPath);
					console.warn('it received', productInfo);
					return;
				}

				this.productInfo = JSON.parse(JSON.stringify(productInfo));
				this.init();

			}.bind(this))
			.fail(function(error) {
				console.warn('buy-module failed on ', productPath);
				// console.error(error);
			});
	}
};



// The user changed the colour in the Buy Module. Update this to match
BuyModule.prototype.onExternalColorChange = function(e, productPath, colorId) {
	if(productPath === this.productPath) {
		var $swatch = this.$colorSwatches.filter('[data-color-id="' + colorId + '"]');
		if($swatch.hasClass('active') !== true && $swatch.hasClass('disabled') !== true) {
			$swatch.trigger('click');
		}
	}
};



// The user changed the model in the Buy Module. Update this to match
BuyModule.prototype.onExternalModelChange = function(e, productPath, key, value) {
	if(productPath === this.productPath) {
		var $dropdown = this.$modelDropdowns.filter('[data-facet-key="' + key + '"]').first();

		if($dropdown.val() !== value) {
			var $option = $dropdown.find('option[value="' + value + '"]');

			if($option.prop('disabled') !== true) {
				$dropdown.val(value);
				$dropdown.trigger('change');
			}
		}
	}
};



BuyModule.prototype.attemptSingleSkuAnalytics = function() {
	if(this.allowTrackSingleSKU === false) {
		return;
	}

	// If the user is looking at a single SKU
	if(Array.isArray(this.currentSKUs) && this.currentSKUs.length === 1) {
		var sku = this.currentSKUs[0];
		// And it's not the same SKU we previously tracked
		if(sku !== this.prevSingleSKU) {
			// Get the price data for it
			this.store.getPriceDataFor(this.productPath, sku)
			.then(function(data) {
				// and track it!
				var listPrice = data.prices.list.amount;
				var unitPrice = data.prices.sale ? data.prices.sale.amount : listPrice;
				var currency  = data.prices.list.currency;
				Analytics.trackViewingSingleSKU(sku, this.productInfo.uniqueId, this.productInfo.category, this.productInfo.title, listPrice, unitPrice, currency);
			}.bind(this));
		}
		this.prevSingleSKU = sku;
	}
};



/**
 * A specific SKU was passed as a hash in the url. Update the colour swatches, model dropdowns, etc... as if the user chose that specific SKU
 * @param  {String} sku
 */
BuyModule.prototype.deepLinkTo = function(sku) {
	var variant = null;

	// First, make sure this SKU actually exists for this product
	for(var i=0; i < this.numVariants; i++) {
		variant = this.productInfo.variants[i];
		if(variant.sku === sku) {
			break;
		} else {
			variant = null;
		}
	}

	if(variant === null) {
		console.warn('No matching variant found for deeplinking to sku ' + sku + ', defaulting to first variant.');
		return;
	}

	// Choose the right colour swatch
	if(this.hasColorChoices) {
		var colorId = variant.facets['dr.color'].id;
		if(colorId !== this.currentColor) {
			this.ignoreColorChangeEvent = true;
			this.currentColor = colorId;
			this.$colorControls.trigger( GLOBALS.colorSwatches.api.CHANGE, [colorId] );
		}
	}

	// Update the model dropdowns
	for(var facetKey in variant.facets) {
		if(facetKey !== 'dr.color' && variant.facets[facetKey] !== undefined) {
			var id = variant.facets[facetKey].id;
			this.currentModels[facetKey] = id;
			var $dropdown = this.$modelDropdowns.filter('[data-facet-key="' + facetKey + '"]');
			$dropdown.val(id);
		}
	}

	if(this.$modelDropdowns.length > 0) {
		this.updateModelDropdowns2(this.currentColor);
	}

	if(this.$modelDropdowns.length > 1) {
		this.updateModelDropdowns();
	}

	if(this.hasColorChoices) {
		this.updateColorSwatches();
	}
};



/**
 * A new colour swatch has been chosen
 */
BuyModule.prototype.onChangeColor = function(e, colorId, fromClick) {
	if (fromClick) {
		this.ignoreColorChangeEvent = false;
	}

	if(this.ignoreColorChangeEvent) {
		this.ignoreColorChangeEvent = false;
		return;
	}
	this.currentColor = colorId;

	if(this.$modelDropdowns.length > 0) {
		this.updateModelDropdowns2(this.currentColor);
	}

	this.updatePriceAndCta();
	this.updateProductShots();
	this.attemptSingleSkuAnalytics();

	this.$elm.trigger(GLOBALS.buyModule.events.COLOR_CHANGE, [this.productPath, colorId]);
};



/**
 * A new model has been chosen
 */
BuyModule.prototype.onChangeModel = function(e) {
	var $dropdown = $(e.currentTarget);

	var key       = $dropdown.attr('data-facet-key');
	var choice    = $dropdown.val();

	if(choice === '') {
		choice = null;
		delete this.currentModels[key];
	} else {
		this.currentModels[key] = choice;
	}

	if(this.$modelDropdowns.length > 1) {
		this.updateModelDropdowns(key, choice);
	}

	if(this.hasColorChoices) {
		this.updateColorSwatches();
	}

	this.updatePriceAndCta();
	this.updateProductShots();
	this.attemptSingleSkuAnalytics();

	this.$elm.trigger(GLOBALS.buyModule.events.MODEL_CHANGE, [this.productPath, key, choice]);
};



/**
 * The user clicked the Add To Cart button
 */
BuyModule.prototype.clickedAddToCart = function(e) {
	if(this.$cta.hasClass('disabled')) {
		this.showModelError();
	}
};

BuyModule.prototype.onAddingToCart = function(e) {
	this.$window.trigger(GLOBALS.modal.api.CLOSE, [true]);
};



BuyModule.prototype.showModelError = function() {
	var winW = this.$window.width();
	var extraClass = (winW < BuyModule.TABLET_BP) ? 'inline-tooltip' : null;
	var $firstWithError = null;

	this.$modelDropdowns.each(function(i, elm) {
		var $dropdown = $(elm);

		if($dropdown.val() === '') {
			var errorExists = false;

			if($firstWithError === null) {
				$firstWithError = $dropdown;
				$dropdown.focus().removeClass('focus--mouse');
			}

			// Just in case, make sure we don't already have an error associated with this dropdown
			for(var i=0; i < this.modelErrorTooltips.length; i++) {
				var tooltip = this.modelErrorTooltips[i];
				if(tooltip.options.$target[0] === elm) {
					errorExists = true;
					break;
				}
			}

			if(errorExists === false) {
				var facetKey = $dropdown.attr('data-facet-key');
				var errorTxt = $dropdown.attr('data-error-text');

				var tooltip = new Tooltip({
					$target: $dropdown,
					anchorTo: Tooltip.ANCHOR_TO_LEFT,
					extraClasses: extraClass,
					text: errorTxt,
					maxWidth: 160,
					padding: 10,
					killOnRolloffTarget: false
				});

				if(winW < BuyModule.TABLET_BP) {
					$dropdown.parent().after(tooltip.$tooltip);
				}

				$dropdown.one('change', this.killModelErrorListener);

				this.modelErrorTooltips.push(tooltip);
			}
		}
	}.bind(this));

	setTimeout(function() {
		this.$window.one('mousedown touchstart', this.killModelErrors.bind(this));
	}.bind(this), 10);
};

BuyModule.prototype.killModelErrors = function(e) {
	if(this.bp === 'mobile') {
		return;
	}

	for(var i=0; i < this.modelErrorTooltips.length; i++) {
		this.modelErrorTooltips[i].kill();
	}

	this.$modelDropdowns.each(function(i, elm) {
		$(elm).off('change', this.killModelErrorListener);
	}.bind(this));

	this.modelErrorTooltips = [];
};

BuyModule.prototype.killModelError = function(e) {
	var dropdown = e.currentTarget;

	for(var i=0; i < this.modelErrorTooltips.length; i++) {
		var tooltip = this.modelErrorTooltips[i];
		if(tooltip.options.$target[0] === dropdown) {
			tooltip.kill();
			this.modelErrorTooltips.splice(i, 1);
			return;
		}
	}
};


/**
 * When we choose a new model we need to make sure that the currently selected colour is an available choice
 * If not, we deselect the current colour and instead select the first available choice
 * We also enable and disable relevant colour choices
 */
BuyModule.prototype.updateColorSwatches = function() {
	var validColors      = [];
	var forceColorChange = false;

	// Loop through all variants
	for(var i=0; i < this.numVariants; i++) {
		var variant = this.productInfo.variants[i];
		var valid   = true;
		var colorId = variant.facets['dr.color'].id;

		// Loop through all currently chosen models
		for(var key in this.currentModels) {
			// Invalid choice
			if(variant.facets[key] !== undefined && variant.facets[key].id !== this.currentModels[key]) {
				valid = false;
				break;
			}
		}

		if(valid) {
			validColors.push(colorId);
		}
	}

	validColors = _.uniq(validColors);

	this.$colorControls.trigger(GLOBALS.colorSwatches.api.SET_VALID_CHOICES, [validColors]);

	// If the currently selected colour isn't a valid choice we need to select a new one
	if(_.indexOf(validColors, this.currentColor) === -1) {
		this.ignoreColorChangeEvent = true; // because we temporarily don't want to respond to the Colour Swatches CHANGE event
		this.currentColor = this.$colorSwatches.filter(':not(.disabled)').first().attr('data-color-id');
		this.$colorControls.trigger(GLOBALS.colorSwatches.api.CHANGE, [this.currentColor]);
	}
};



BuyModule.prototype.updateModelDropdowns = function(latestModelKey, latestModelChoice) {
	var unavailInKey;

	// Loop through model dropdowns
	for(var i=0; i < this.$modelDropdowns.length; i++) {
		var $modelDropdown = this.$modelDropdowns.eq(i);
		var thisFacetKey   = $modelDropdown.attr('data-facet-key');
		var thisVal        = $modelDropdown.val();

		// If this model dropdown is not the one the user just updated, or if the latestChoice was de-selecting something
		if(thisFacetKey !== latestModelKey || latestModelChoice === '') {
			var $options = $modelDropdown.find('option');

			// Loop over each option in this dropdown
			for(var n=1; n < $options.length; n++) {
				var $option      = $options.eq(n);
				var optionVal    = $option.val();
				var label        = $option.attr('data-label');
				var variantMatch;

				// Loop over all variants and see if there's an overlap between this option and all other choices
				for(var q=0; q < this.productInfo.variants.length; q++) {
					var variant  = this.productInfo.variants[q];
					variantMatch = true; // assume its a match until proven otherwise

					// Loop through all currently chosen models
					for(var key in this.currentModels) {
						// Invalid choice
						if(key !== thisFacetKey && variant.facets[key] !== undefined && variant.facets[key].id !== this.currentModels[key]) {
							unavailInKey = key;
							variantMatch = false;
							break;
						}
					}

					if(variant.facets[thisFacetKey] !== undefined && variant.facets[thisFacetKey].id !== optionVal) {
						variantMatch = false;
					}

					if(variantMatch === true) {
						break;
					}
				}

				if(variantMatch === true) {
					$option.prop('disabled', false).html(label);

				// uh oh, this option isn't available with other selected options
				} else {
					var notAvailTxt = I18n.get('facet.' + unavailInKey + '.notAvail');
					if(notAvailTxt === 'facet.' + unavailInKey + '.notAvail') {
						notAvailTxt = I18n.get('facet.notAvail.fallback');
					}

					$option.prop('disabled', true).html( label + ' (' + notAvailTxt + ')');

					// If it happens to currently be selected, deselect it
					if(thisVal !== '' && thisVal === optionVal) {
						$modelDropdown.val('');
						delete this.currentModels[thisFacetKey];
					}
				}
			}
		}
	}
};

BuyModule.prototype.updateModelDropdowns2 = function() {
	if(this.currentColor === null) {
		return;
	}

	var unavailInKey = 'dr.color';

	// Loop through model dropdowns
	for(var i=0; i < this.$modelDropdowns.length; i++) {
		var $modelDropdown = this.$modelDropdowns.eq(i);
		var thisFacetKey   = $modelDropdown.attr('data-facet-key');
		var thisVal        = $modelDropdown.val();
		var $options       = $modelDropdown.find('option');

		// Loop over each option in this dropdown
		for(var n=1; n < $options.length; n++) {
			var $option      = $options.eq(n);
			var optionVal    = $option.val();
			var label        = $option.attr('data-label');
			var variantMatch = false;

			// Loop over all variants and see if this model is available in the chosen colour
			for(var q=0; q < this.productInfo.variants.length; q++) {
				var variant  = this.productInfo.variants[q];

				if(variant.facets['dr.color'].id === this.currentColor && variant.facets[thisFacetKey].id === optionVal) {
					variantMatch = true;
					break;
				}
			}

			if(variantMatch === true) {
				$option.prop('disabled', false).html(label);

			// uh oh, this option isn't available with other selected options
			} else {
				var notAvailTxt = I18n.get('facet.' + unavailInKey + '.notAvail');
				if(notAvailTxt === 'facet.' + unavailInKey + '.notAvail') {
					notAvailTxt = I18n.get('facet.notAvail.fallback');
				}

				$option.prop('disabled', true).html( label + ' (' + notAvailTxt + ')');

				// If it happens to currently be selected, deselect it
				if(thisVal !== '' && thisVal === optionVal) {
					$modelDropdown.val('');
					delete this.currentModels[thisFacetKey];
				}
			}
		}
	}
};



/**
 * Get a list of SKUs that match the current color and model and set it on the price and cta divs
 */
BuyModule.prototype.updatePriceAndCta = function() {
	var skus           = [];
	var productPath    = null;

	for(var i=0; i < this.numVariants; i++) {
		var variant = this.productInfo.variants[i];
		var sku     = variant.sku;
		var match   = true;
		var colorId;

		try {
			colorId = variant.facets['dr.color'].id;
		} catch(error) {
			colorId = null;
		}

		// Skip over marketing-only variants
		// if(variant.buyStatus === '4') {
		// 	continue;
		// }

		// If there's a color chosen and it doesn't match this variant's color, not a match
		if(this.currentColor !== null && this.currentColor !== colorId) {
			match = false;
		}

		// Loop through all currently chosen models
		for(var key in this.currentModels) {
			if(variant.facets[key] !== undefined && variant.facets[key].id !== this.currentModels[key]) {
				match = false;
				break;
			}
		}

		if(match) {
			skus.push(sku);

			// The product path just gets set to whatever the first match is
			if(productPath === null) {
				productPath = variant.link;
			}
		}
	}

	// console.log('skus', skus);

	if(skus.length === 0) {
		// no matches were found. Likely this is because there's neither colour nor model choices.
		// fallback to include all skus

		if(this.numVariants > 1) {
			console.warn('Warning! No SKU matches were found based on chosen model and/or colour.');
		}

		skus = this.productInfo.skus;
		productPath = this.productInfo.link;
	}

	this.currentSKUs = skus;

	skus = skus.join(',');

	this.$price.attr('data-skus', skus);
	this.$price.attr('data-product-path', productPath);
	this.$cta.attr('data-skus', skus);
	this.$cta.attr('data-product-path', productPath);

	// Listen for the price/cta update
	this.priceUpdateComplete = false;
	this.ctaUpdateComplete   = false;
	this.$price.off( GLOBALS.productPrice.events.UPDATED );
	this.$cta.off( GLOBALS.productCta.events.UPDATED );
	this.$price.on( GLOBALS.productPrice.events.UPDATED, this.onUpdatedPrice.bind(this) );
	this.$cta.on( GLOBALS.productCta.events.UPDATED, this.onUpdatedCta.bind(this) );



	// Should we disable the CTA?
	if(this.currentSKUs.length > 1 || (this.hasModelChoices && this.areAllModelDropdownsSelected() !== true)) {
		this.$cta.addClass('disabled');
		this.$onlineRetailersTrigger.addClass('disabled');
		if(this.numVariants > 1) {
			Utils.setSelector(null);
		}

	} else {
		this.$cta.removeClass('disabled');
		this.$onlineRetailersTrigger.removeClass('disabled');
		if(this.numVariants > 1 && this.setSelectorAllowed === true) {
			Utils.setSelector(this.currentSKUs[0]);
		}
		if(this.currentSKUs.length === 1) {
			var variant = null;
			// Make sure SKU actually exists for this product
			for(var i=0; i < this.numVariants; i++) {
				variant = this.productInfo.variants[i];
				if(variant.sku === this.currentSKUs[0]) {
					break;
				}
			}

			if(variant === null) {
				return;
			}

			if(variant.variantAnalytics !== undefined) {
				this.$elm.trigger( GLOBALS.productCta.api.UPDATE_ANALYTICS_ID, [variant.variantAnalytics]);
			} else {
				this.$elm.trigger( GLOBALS.productCta.api.UPDATE_ANALYTICS_ID, [null]);
			}

			if(this.setSelectorAllowed === true) {
				Utils.setSelector(this.currentSKUs[0]);
			}
		}
	}

	// if limited edition, start polling for stock quantity
	// see BUY_STATUS in cta-manager.js for status codes

	if (this.numVariants > 0) {
		if (this.moduleType === 'limited-edition' && this.productInfo.variants[0].buyStatus === 1) {
			this.pollStock();
		} else if (this.productInfo.variants[0].buyStatus === 2 || this.productInfo.variants[0].buyStatus === 3) {
			this.$cta.addClass('visible');
		}
	}

	// Trigger the price and cta to update
	this.$price.trigger( GLOBALS.productPrice.api.UPDATE );
	this.$cta.trigger( GLOBALS.productCta.api.UPDATE );
};


BuyModule.prototype.pollStock = function(e, price) {

	console.log('Refreshing stock status');

	var that = this;
	this.store.getStockCountFor( this.productPath, this.currentSKUs, this.showProductCount )
		.then(function(response){

			// example response:
			// { "910-005469": 10000 }

			// convert response text to JSON
			if( typeof response == 'string' ) response = JSON.parse(response);

			var sku   = that.currentSKUs[0];

			var count = response[sku];
			if( that.showProductCount == true ){
				count = parseInt( response[sku] );
			}


			if( !count && count !== 0 ){
				console.log('Error retrieving product inventory count.');
				return;
			}

			// schedule next inventory refresh
			// 60 seconds if count > 100 or h for "high"
			// 10 seconds if count < 100 of l for "low"
			var waitTime = ( count > 100 || count == 'h' ) ? 60000 : 10000;
			if( count !== 0 && count !== '0' ){
				setTimeout(function(){
					that.pollStock();
				},waitTime);
			}

			// show CTA beacuse we know stock is available
			// note: CTA innerHTML may be transformed below
			that.$cta.addClass('visible');

			if( count > 1000 ) return;

			var remainingMessage      = '';
			var remainingMessageClass = 'units-remaining';

			// under 1000 left
			if( count <= 1000 && count > 10 && that.showProductCount == true ){
				remainingMessage = I18n.get('le.few.left', count);
			}

			// sold out
			if( count <= 10 || count == '0' ){
				soldOutText            = I18n.get('sold.out');
				remainingMessage       = I18n.get('le.zero.left');
				remainingMessageClass += ' sold-out';

				// replace cta button with Sold Out text
				that.$cta.html('<div class="product-cta-note out-of-stock "><span>'+soldOutText+'</span></div>');

			}

			$('.units-remaining').remove();

			// insert message
			$('<span>'+remainingMessage+'</span>')
				.addClass( remainingMessageClass )
				.insertAfter( that.$elm.find('.product-pricing-info') );
		});

};

BuyModule.prototype.onUpdatedPrice = function(e, price) {
	this.priceUpdateComplete = price;
	// console.log('this.priceUpdateComplete', this.priceUpdateComplete);
	// this.$price.off( GLOBALS.productPrice.events.UPDATED );

	if(this.ctaUpdateComplete !== false) {
		this.updateOnlineRetailers();
	}
